{
    "name": "src",
    "version": "1.0.0",
    "private": true,
    "scripts": {
        "dev": "npm-run-all i18n:flatten buildDynamicImports:* && npm-run-all -p watch:components next:dev",
        "dist": "npm-run-all i18n:flatten buildDynamicImports:* clear:dist:next clear:dist:build && npx next build",
        "build": "npm-run-all i18n:flatten buildDynamicImports:* next:build",
        "start": "npm-run-all i18n:flatten buildDynamicImports:* next:start",
        "export": "npx next build && npx next export",
        "clear:dist:next": "rimraf ./.next",
        "clear:dist:build": "rimraf ./.next",
        "i18n:flatten": "node ./tasks/i18n.flatten.js",
        "buildDynamicImports:project": "node ./tasks/dynamic.project.imports.js",
        "buildDynamicImports:icons": "node ./tasks/dynamic.icons.imports.js",
        "watch:components": "nodemon --watch 'components/**/*' -e js --exec 'npm run buildDynamicImports:project'",
        "next:dev": "next dev",
        "next:build": "next build",
        "next:start": "next start"
    },
    "dependencies": {
        "@carbon/icons-react": "^10.30.0",
        "@carbon/pictograms-react": "^11.8.0",
        "@carbon/type": "^10.26.0",
        "carbon-components": "^10.33.0",
        "carbon-components-react": "^7.33.0",
        "carbon-icons": "^7.0.7",
        "clsx": "^1.1.1",
        "intersection-observer": "^0.12.0",
        "next": "12.0.9",
        "react": "17.0.2",
        "react-animate-height": "^2.0.23",
        "react-dom": "17.0.2",
        "react-in-viewport": "^1.0.0-alpha.20",
        "react-intl": "^5.16.0",
        "react-sticky-el": "^2.0.7",
        "react-stickynode": "^3.0.5",
        "react-transition-group": "^4.4.2",
        "sass": "^1.32.11"
    },
    "devDependencies": {
        "babel-eslint": "^10.1.0",
        "cpy-cli": "^3.1.1",
        "eslint": "^7.1.0",
        "eslint-config-prettier": "^8.2.0",
        "eslint-config-react": "^1.1.7",
        "eslint-config-standard": "^16.0.2",
        "eslint-plugin-import": "^2.20.2",
        "eslint-plugin-node": "^11.1.0",
        "eslint-plugin-prettier": "^3.1.3",
        "eslint-plugin-promise": "^5.1.0",
        "eslint-plugin-react": "^7.20.0",
        "eslint-plugin-standard": "^5.0.0",
        "eslint-plugin-unused-imports": "^1.1.1",
        "file-loader": "^6.0.0",
        "glob": "^7.1.6",
        "html-entities": "^2.3.2",
        "lodash": "^4.17.21",
        "markdown-to-jsx": "^7.0.0",
        "nodemon": "^2.0.7",
        "npm-run-all": "^4.1.5",
        "prettier": "^2.0.5",
        "prettier-eslint": "^12.0.0",
        "prettier-eslint-cli": "^5.0.0",
        "prop-types": "^15.7.2",
        "rimraf": "^3.0.2",
        "shelljs": "^0.8.5",
        "stylelint": "^13.5.0",
        "stylelint-config-standard": "^21.0.0",
        "stylelint-scss": "^3.19.0"
    }
}
